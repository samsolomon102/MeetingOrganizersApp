<p style="color: green"><%= notice %></p>

<h1>Employee list</h1>

<div id="meeting_organizer_lists">
  <table class="table table-hover">
    <thead class="table-dark">
      <th>
        Employee ID
      </th>
      <th>
        First Name
      </th>
      <th>
        Last Name
      </th>
      <th>
        Email ID
      </th>
      <th>
        Last Scrum Master
      </th>
      <th colspan="3"></th>
    </thead>
    <% @meeting_organizer_lists.each do |meeting_organizer| %>
      <tr>
        <td>
          <%=meeting_organizer.employee_id%>
        </td>
        <td>
          <%=meeting_organizer.first_name%>
        </td>
        <td>
          <%=meeting_organizer.last_name%>
        </td>
        <td>
          <%=meeting_organizer.email_id%>
        </td>
        <td>
          <%=meeting_organizer.last_used%>
        </td>
        <td>
          <%= link_to "Show", meeting_organizer, class:"btn btn-outline-secondary" %>
        </td>
        <td>
          <%= link_to "Edit", "/meeting_organizer_lists/"+meeting_organizer.id.to_s+"/edit", class:"btn btn-outline-secondary" %>
        </td>
        <td>
          <%= button_to "Delete", meeting_organizer, :confirm => "Are you sure?", method: :delete, class:"btn btn-outline-secondary"%>
        </td>
      </tr>
    <% end %>
  </table>
</div>

<%= link_to "New meeting organizer list", new_meeting_organizer_list_path, class:"btn btn-dark" %>
